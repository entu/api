{
  "name": "entu-api",
  "version": "3.0.0",
  "private": true,
  "description": "Entu API",
  "author": "Argo Roots <argo@roots.ee>",
  "license": "MIT",
  "scripts": {
    "dev": "npm ci --quiet --production --prefix layers/nodejs && sam local start-api --env-vars env.json --warm-containers LAZY --parameter-overrides Architecture=x86_64",
